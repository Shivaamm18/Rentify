{
  "buildCommand": "cd client && npm install && CI=false GENERATE_SOURCEMAP=false npm run build",
  "outputDirectory": "client/build",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://rentify-server.onrender.com/api/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
